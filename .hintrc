{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 116",
    "not chrome <= 116",
    "not edge <= 116",
    "not firefox <= 117",
    "not safari <= 16.5",
    "not samsung <= 21"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scale"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}